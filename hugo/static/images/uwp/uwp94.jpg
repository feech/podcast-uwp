}else
{
window.location.href = orderStatusURLnotLogin;
} 
} 
else{
if(remember != null && remember == '1' && !isloggedIn)
{
window.location.href = orderStatusURLnotLogin;
}
else if (!isloggedIn)
{
window.location.href = orderStatusURLnotLogin;
}else
{
window.location.href = orderStatusURLLogin;
} 
} 
}
